<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                        http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
  <!-- Capabilities -->
  <servlet>
    <servlet-name>CapabilitiesServlet</servlet-name>
    <servlet-class>com.appspot.capabilities.CapabilitiesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CapabilitiesServlet</servlet-name>
    <url-pattern>/capabilities</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogServlet</servlet-name>
    <servlet-class>com.appspot.capabilities.LogServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogServlet</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <!-- XMPP -->
  <servlet>
    <servlet-name>SendXMPPServlet</servlet-name>
    <servlet-class>com.appspot.xmpp.SendXMPPServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SendXMPPServlet</servlet-name>
    <url-pattern>/send-xmpp</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>XMPPMessageServlet</servlet-name>
    <servlet-class>com.appspot.xmpp.XMPPMessageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>XMPPMessageServlet</servlet-name>
    <url-pattern>/_ah/xmpp/message/chat/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>XMPPPresenceServlet</servlet-name>
    <servlet-class>com.appspot.xmpp.XMPPPresenceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>XMPPPresenceServlet</servlet-name>
    <url-pattern>/_ah/xmpp/presence/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>XMPPSubscriptionServlet</servlet-name>
    <servlet-class>com.appspot.xmpp.XMPPSubscriptionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>XMPPSubscriptionServlet</servlet-name>
    <url-pattern>/_ah/xmpp/subscription/*</url-pattern>
  </servlet-mapping>
  <!-- Security -->
  <servlet>
    <servlet-name>CheckUserServlet</servlet-name>
    <servlet-class>com.appspot.security.CheckUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckUserServlet</servlet-name>
    <url-pattern>/check-user</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>CheckFederatedServlet</servlet-name>
    <servlet-class>
      com.appspot.security.CheckFederatedServlet
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckFederatedServlet</servlet-name>
    <url-pattern>/check-federated</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CheckOAuthServlet</servlet-name>
    <servlet-class>
      com.appspot.security.CheckOAuthServlet
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckOAuthServlet</servlet-name>
    <url-pattern>/check-oauth</url-pattern>
  </servlet-mapping>
  <!-- Data store -->
  <servlet>
		<servlet-name>RemoteApiServlet</servlet-name>
		<servlet-class>com.google.apphosting.utils.remoteapi.RemoteApiServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RemoteApiServlet</servlet-name>
		<url-pattern>/remote-api</url-pattern>
	</servlet-mapping>
    <servlet>
		<servlet-name>QueryFilesServlet</servlet-name>
		<servlet-class>com.appspot.datastore.QueryFilesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QueryFilesServlet</servlet-name>
		<url-pattern>/query-files</url-pattern>
	</servlet-mapping>
    <servlet>
		<servlet-name>RetrieveBlobServlet</servlet-name>
		<servlet-class>com.appspot.datastore.RetrieveBlobServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RetrieveBlobServlet</servlet-name>
		<url-pattern>/retrieve-blob/*</url-pattern>
	</servlet-mapping>
    <servlet>
		<servlet-name>StoreBlobServlet</servlet-name>
		<servlet-class>com.appspot.datastore.StoreBlobServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StoreBlobServlet</servlet-name>
		<url-pattern>/store-blob</url-pattern>
	</servlet-mapping>
    <servlet>
		<servlet-name>StoreDataServlet</servlet-name>
		<servlet-class>com.appspot.datastore.StoreDataTransactionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StoreDataServlet</servlet-name>
		<url-pattern>/store-data</url-pattern>
	</servlet-mapping>
    <servlet>
		<servlet-name>QueryDataServlet</servlet-name>
		<servlet-class>com.appspot.datastore.QueryDataAsyncServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QueryDataServlet</servlet-name>
		<url-pattern>/query-data</url-pattern>
	</servlet-mapping>
    <servlet>
		<servlet-name>RetrieveDataServlet</servlet-name>
		<servlet-class>com.appspot.datastore.RetrieveDataAsyncServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RetrieveDataServlet</servlet-name>
		<url-pattern>/retrieve-data/*</url-pattern>
	</servlet-mapping>
    <servlet-mapping>
		<servlet-name>RetrieveDataServlet</servlet-name>
		<url-pattern>/retrieve-data</url-pattern>
	</servlet-mapping>
    <!-- URL Fetch -->
  <servlet>
		<servlet-name>OpenChannelServlet</servlet-name>
		<servlet-class>com.appspot.channel.OpenChannelServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OpenChannelServlet</servlet-name>
		<url-pattern>/open-channel</url-pattern>
	</servlet-mapping>
    <servlet>
		<servlet-name>SendMessageServlet</servlet-name>
		<servlet-class>com.appspot.channel.SendMessageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SendMessageServlet</servlet-name>
		<url-pattern>/send-message</url-pattern>
	</servlet-mapping>
  <servlet>
		<servlet-name>ASyncUrlFetchServlet</servlet-name>
		<servlet-class>com.appspot.urlfetch.ASyncUrlFetchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ASyncUrlFetchServlet</servlet-name>
		<url-pattern>/async-url-fetch</url-pattern>
	</servlet-mapping>
  <servlet>
		<servlet-name>SyncUrlFetchServlet</servlet-name>
		<servlet-class>com.appspot.urlfetch.SyncUrlFetchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SyncUrlFetchServlet</servlet-name>
		<url-pattern>/sync-url-fetch</url-pattern>
	</servlet-mapping>
  <servlet>
		<servlet-name>SyncUrlPostServlet</servlet-name>
		<servlet-class>com.appspot.urlfetch.SyncUrlPostServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SyncUrlPostServlet</servlet-name>
		<url-pattern>/sync-url-post</url-pattern>
	</servlet-mapping>

  <!-- Cache -->
	<servlet>
		<servlet-name>ETagCacheServlet</servlet-name>
		<servlet-class>com.appspot.cache.ETagCacheServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ETagCacheServlet</servlet-name>
		<url-pattern>/etag-cache</url-pattern>
	</servlet-mapping>
  <servlet>
		<servlet-name>SimpleCacheServlet</servlet-name>
		<servlet-class>com.appspot.cache.SimpleCacheServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SimpleCacheServlet</servlet-name>
		<url-pattern>/simple-cache</url-pattern>
	</servlet-mapping>
  <servlet>
	<servlet-name>InvalidateCacheServlet</servlet-name>
		<servlet-class>com.appspot.cache.InvalidateCacheServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InvalidateCacheServlet</servlet-name>
		<url-pattern>/invalidate-cache</url-pattern>
	</servlet-mapping>
  <servlet>
	<servlet-name>ClearCacheServlet</servlet-name>
		<servlet-class>com.appspot.cache.ClearCacheServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ClearCacheServlet</servlet-name>
		<url-pattern>/clear-cache</url-pattern>
	</servlet-mapping>

  <!-- Template -->
	<servlet>
		<servlet-name>StringTemplateServlet</servlet-name>
		<servlet-class>com.appspot.template.StringTemplateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StringTemplateServlet</servlet-name>
		<url-pattern>/st</url-pattern>
	</servlet-mapping>

	<!--  Image -->
	<servlet>
		<servlet-name>RetrieveImageServlet</servlet-name>
		<servlet-class>com.appspot.images.RetrieveDatastoreImageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RetrieveImageServlet</servlet-name>
		<url-pattern>/retrieve-image</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ReceiveImageServlet</servlet-name>
		<servlet-class>com.appspot.images.ReceiveImageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReceiveImageServlet</servlet-name>
		<url-pattern>/receive-image</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ResizeImageServlet</servlet-name>
		<servlet-class>com.appspot.images.ResizeImageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResizeImageServlet</servlet-name>
		<url-pattern>/resize-image</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ImageServlet</servlet-name>
		<servlet-class>com.appspot.images.CompositeImageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ImageServlet</servlet-name>
		<url-pattern>/composite-image</url-pattern>
	</servlet-mapping>
		
	<!--  Tasks -->
	<servlet>
		<servlet-name>CronMailServlet</servlet-name>
		<servlet-class>com.appspot.tasks.CronMailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CronMailServlet</servlet-name>
		<url-pattern>/tasks/m</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>QueueMailTaskServlet</servlet-name>
		<servlet-class>com.appspot.tasks.QueueMailTaskServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QueueMailTaskServlet</servlet-name>
		<url-pattern>/tasks/queue/low-level-mail</url-pattern>
	</servlet-mapping>
	

	<!-- Mail -->
	<servlet>
		<servlet-name>SendMailServlet</servlet-name>
		<servlet-class>com.appspot.mail.SendNotificationJMServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SendMailServlet</servlet-name>
		<url-pattern>/mail/send</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SendMultipartMailServlet</servlet-name>
		<servlet-class>com.appspot.mail.SendConfirmationJMServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SendMultipartMailServlet</servlet-name>
		<url-pattern>/mail/send-multipart</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LowLevelMailServlet</servlet-name>
		<servlet-class>com.appspot.mail.SendNotificationLLServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LowLevelMailServlet</servlet-name>
		<url-pattern>/mail/low-level</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LowLevelMultipartMailServlet</servlet-name>
		<servlet-class>com.appspot.mail.SendConfirmationLLServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LowLevelMultipartMailServlet</servlet-name>
		<url-pattern>/mail/low-level-multipart</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ReceiveMailServlet</servlet-name>
		<servlet-class>com.appspot.mail.ReceiveMailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReceiveMailServlet</servlet-name>
		<url-pattern>/_ah/mail/info@blo-gae.appspotmail.com</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ReceiveRawMailServlet</servlet-name>
		<servlet-class>com.appspot.mail.ReceiveMailLLServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReceiveRawMailServlet</servlet-name>
		<url-pattern>/_ah/mail/raw@blo-gae.appspotmail.com</url-pattern>
	</servlet-mapping>
  <servlet>
    <servlet-name>appstats</servlet-name>
    <servlet-class>
      com.google.appengine.tools.appstats.AppstatsServlet
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>appstats</servlet-name>
    <url-pattern>/appstats/*</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>appstats</filter-name>
    <filter-class>
      com.google.appengine.tools.appstats.AppstatsFilter
    </filter-class>
  </filter>
  <filter-mapping>
    <filter-name>appstats</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<security-constraint>
		<web-resource-collection>
			<web-resource-name>store-data-url</web-resource-name>
			<url-pattern>/store-*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>admin</role-name>
		</auth-constraint>
	</security-constraint>
    <security-constraint>
		<web-resource-collection>
			<web-resource-name>receive-mail-url</web-resource-name>
			<url-pattern>/_ah/mail/*</url-pattern>
		</web-resource-collection>
      <auth-constraint>
        <role-name>admin</role-name>
      </auth-constraint>
    </security-constraint>
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>tasks-url</web-resource-name>
      <url-pattern>/tasks/*</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-name>admin</role-name>
    </auth-constraint>
  </security-constraint>
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>send-mail-url</web-resource-name>
      <url-pattern>/mail/*</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-name>admin</role-name>
    </auth-constraint>
  </security-constraint>
  <servlet>
    <servlet-name>appstats</servlet-name>
    <servlet-class>com.google.appengine.tools.appstats.AppstatsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>appstats</servlet-name>
    <url-pattern>/appstats/*</url-pattern>
  </servlet-mapping>
</web-app>
